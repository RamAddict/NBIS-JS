{
  "compilerOptions": {
    "target": "ES2022",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "bundler",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*.ts", "types/*.d.ts", "types/nbis-wasm.d.ts", "types/nbis-wasm.d.ts"]
}
// if vscode complains about this file, just ignore it
// or change the typescript version to use workspace version
